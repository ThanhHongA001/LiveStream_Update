<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LiveStream.MainActivity_Channel_Detail.MainActivity_Channel_Detail_Fragment02">

    <androidx.swiperefreshlayout.widget.SwipeRefreshLayout
        android:id="@+id/rm_activity_main_channel_detail_fragment02_srl_01"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/rm_activity_main_channel_detail_fragment02_rv_01"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
    </androidx.swiperefreshlayout.widget.SwipeRefreshLayout>

    <LinearLayout
        android:id="@+id/rm_activity_main_channel_detail_fragment02_ll_01"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:layout_marginTop="70dp"
        android:visibility="gone">

        <include
            android:id="@+id/rm_activity_main_channel_detail_fragment02_inc_01"
            layout="@layout/rm_activity_item08" />
    </LinearLayout>

</FrameLayout>
